<testcase xmlns="tag:dmaus@dmaus.name,2019:Schematron:Testsuite" expectValid="false" features="svrl" id="svrl-diagnostic">
  <label>Diagnostic references are copied to SVRL output</label>
  <documents>
    <document id="primary" filename="document.xml">
      <element xmlns=""/>
    </document>
  </documents>
  <schemas document="primary">
    <sch:schema xmlns:sch="http://purl.oclc.org/dsdl/schematron">
      <sch:pattern>
        <sch:rule context="/">
          <sch:report test="true()" diagnostics="d1 d2"/>
        </sch:rule>
      </sch:pattern>
      <sch:diagnostics>
        <sch:diagnostic id="d1">
          Context: <sch:value-of select="name()"/>
        </sch:diagnostic>
        <sch:diagnostic id="d2">
          Context: <sch:value-of select="name()"/>
        </sch:diagnostic>
      </sch:diagnostics>
    </sch:schema>
  </schemas>
  <expectations>
    <expectation test="count(//svrl:successful-report/svrl:diagnostic-reference[@diagnostic = 'd1' or @diagnostic = 'd2']) = 2"/>
  </expectations>
</testcase>
